{
  "manifest_version": 2,
  "name": "Tocket",
  "version": "1.0",
  "description": "Get 5 old items from Pocket",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "icons" : {
    "128": "icon.png"
  }
}
